#find the area of rectangle 
l=92
b=48.8
area=l*b
print(area)
print("The area is:",area)

#division operator example 
x=15
y=6
print(x/y)

#floor division example 
x=15
y=6
print(x//y)

#print square and cube of number 
a=int(input("enter the number:"))
print("The square is:"a**2)
print("The cube is:"a**3)

#AND operator 
a=10
b=10
if a>0 and b>0:
  print("The number is greater than 0")

#OR operator 
a=10
b=-10
if a>0 or b>0:
print("Either the number is greater than o")
else:
print("No number is greater than 0")

#NOT Operator 
a=10
if not a:
 print("Boolen value of a is true")
if not(a%3==0 or a%5==0)
 print ("10 is not divisible by either 3 or 5")
else:
 print("10 is divisible by either 3 or 5")

#Average marks of student 
a=int(input("Enter the Maths Marks:"))
b=int(input("Enter the Physics Marks:"))
c=int(input("Enter the Chemistry Marks:"))
d=int(input("Enter the Biology Marks:"))
e=a+b+c+d/4
print(e)
